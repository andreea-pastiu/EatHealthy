@using EatHealthy.DTO
@using EatHealthy.Models
@model PatientsDTO

@if (Model.Patients != null)
{
    for (int i = 0; i < Model.Patients.Count; i++)
    {
        Patient patient = Model.Patients[i];
        <div>
            Patient: @patient.Name <br />
            Weight: @patient.Weight <br />
            Height: @patient.Height <br />
            BMI: @patient.BMI <br />
        </div>
    }
}